{
  "name": "el-dorado-tanstack",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.1.1",
  "scripts": {
    "dev": "pnpm run dev:stack",
    "dev:stack": "concurrently --raw \"PORT=4000 pnpm --filter @game/server dev:watch\" \"VITE_API_URL=http://localhost:4000 VITE_WS_URL=ws://localhost:4000/ws pnpm --filter @game/web dev\"",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "lint": "pnpm -r run lint"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^24.10.1",
    "chrome-mcp": "^1.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
